{
  "name": "mooc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "console:dev": "pnpm --filter console dev",
    "server:dev": "pnpm --filter server dev",
    "web:dev": "pnpm --filter web dev:web",
    "server:build": "pnpm --filter server build",
    "console:build": "pnpm --filter console build",
    "web:build": "pnpm --filter web build",
    "server:start": "pnpm --filter server start",
    "console:start": "pnpm --filter console start",
    "web:start": "pnpm --filter web start",
    "server:lint": "pnpm --filter server lint",
    "console:lint": "pnpm --filter console lint",
    "web:lint": "pnpm --filter web lint",
    "build": "pnpm run server:build & pnpm run console:build & pnpm run web:build",
    "lint": "pnpm run server:lint && pnpm run console:lint && pnpm run web:lint && pnpm --filter db-shared lint && pnpm --filter live-service lint",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "mediasoup": "^3.16.6",
    "react-dom": "^19.2.3",
    "redis": "^5.6.0",
    "rollup-plugin-copy": "^3.5.0",
    "winston": "^3.17.0"
  },
  "lint-staged": {
    "console/**/*.{ts,tsx,js,jsx}": [
      "pnpm --filter console lint"
    ],
    "server/**/*.{ts,tsx,js,jsx}": [
      "pnpm --filter server lint"
    ],
    "web/**/*.{ts,tsx,js,jsx}": [
      "pnpm --filter web lint"
    ],
    "packages/db-shared/**/*.{ts,tsx,js,jsx}": [
      "pnpm --filter @mooc/db-shared lint"
    ],
    "packages/live-service/**/*.{ts,tsx,js,jsx}": [
      "pnpm --filter @mooc/live-service lint"
    ],
    "*.{json,js,ts,md}": [
      "echo 'Root configuration files changed - consider running full lint check'"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.0.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.35.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.0",
    "webpack": "^5.101.1"
  }
}
